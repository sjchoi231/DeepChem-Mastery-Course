      "cell_type": "code",
      "source": [
        "def preprocess_smiles(smiles):\n",
        "  return sf.encoder(smiles)\n",
        "\n",
        "def keys_int(symbol_to_int):\n",
        "  d={}\n",
        "  i=0\n",
        "  for key in symbol_to_int.keys():\n",
        "    d[i]=key\n",
        "    i+=1\n",
        "  return d\n",
        "\n",
        "data['selfies'] = data['smiles'].apply(preprocess_smiles)"
